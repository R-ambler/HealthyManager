<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <display-name></display-name>
  <servlet>
    <servlet-name>SigninHandler</servlet-name>
    <servlet-class>HM.Handler.Signin.SigninHandler</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoginHandler</servlet-name>
    <servlet-class>HM.Handler.Login.LoginHandler</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>FindPasswordHandler</servlet-name>
    <servlet-class>HM.Handler.FindPassword.FindPasswordHandler</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SearchHandler</servlet-name>
    <servlet-class>HM.Handler.Search.SearchHandler</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>MedicalHandler</servlet-name>
    <servlet-class>HM.Handler.Medical.MedicalHandler</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>MedicineHandler</servlet-name>
    <servlet-class>HM.Handler.Medicine.MedicineHandler</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>PrescriptionHandler</servlet-name>
    <servlet-class>HM.Handler.Prescription.PrescriptionHandler</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AppService</servlet-name>
    <servlet-class>app.controller.AppService</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>SigninHandler</servlet-name>
    <url-pattern>/SigninHandler</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginHandler</servlet-name>
    <url-pattern>/LoginHandler</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FindPasswordHandler</servlet-name>
    <url-pattern>/FindPasswordHandler</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SearchHandler</servlet-name>
    <url-pattern>/SearchHandler</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MedicalHandler</servlet-name>
    <url-pattern>/MedicalHandler</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PrescriptionHandler</servlet-name>
    <url-pattern>/PrescriptionHandler</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AppService</servlet-name>
    <url-pattern>/AppService</url-pattern>
  </servlet-mapping>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
